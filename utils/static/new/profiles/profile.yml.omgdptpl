{{- if .profile.game }}
game:
  {{- range $key, $value := .profile.game }}
  {{ $key }}: {{ $value  }}
  {{- end }}
{{- end }}


{{ if .profile.omgd.deploy.server.gcloud }}
gcloud:
  {{- range $key, $value := .profile.omgd.deploy.server.gcloud }}
  {{ $key }}: {{ $value  }}
  {{- end }}
{{- end }}


nakama:
{{- if .profile.omgd.deploy }}
  host: ???
{{- else }}
  host: 127.0.0.1
{{- end }}
  port: 7350
  secure: false
  key: default-key

